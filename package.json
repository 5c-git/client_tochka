    "babel-loader": "^8.2.2",
    "babel-preset-airbnb": "^4.4.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.11",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.20.1",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^4.5.2",
